<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.meee.remeber.remembermeee">
    <uses-permission android:name="android.permission.INTERNET" />
    <!--minimal sdk version in library xrecyclerview is 16, hight than 15 here.-->
    <!--use this line to force use xrecyclerview library.-->
    <uses-sdk tools:overrideLibrary="com.example.xrecyclerview"/>
    <application
        android:name=".app.RememberMeeeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:overrideLibrary="com.example.xrecyclerview"
        >

        <activity android:name=".ui.TransitionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".ui.menu.NavHomePageActivity"/>

    </application>

</manifest>